{
  "name": "@gapotchenko/deno-cgi",
  "version": "1.0.0",
  "license": "MPL-2.0",
  "exports": "./mod.ts",
  "imports": {
    "@std/streams": "jsr:@std/streams@^1.0.12"
  },
  "publish": {
    "exclude": [
      "examples/",
      "CHANGELOG.md",
      "justfile",
      "*.code-workspace"
    ]
